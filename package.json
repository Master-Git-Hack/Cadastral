{
  "name": "nextjs-fastapi",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "backend": "pip3 install -r requirements.txt && python3 -m uvicorn api.main:app --host 0.0.0.0 --port 5000 --reload --log-level debug --use-colors",
    "frontend": "next dev --turbo --port 3000",
    "format": "concurrently \"black ./api/\" \"prettier --ignore-path ./.prettierignore --config ./.prettierrc.json --write './**/*.{ts,tsx,}'\"",
    "dev": "concurrently \"yarn frontend\" \"yarn backend\"",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "next lint"
  },
  "dependencies": {
    "@heroicons/react": "^2.1.5",
    "@hookform/resolvers": "^3.9.0",
    "@radix-ui/react-alert-dialog": "^1.1.1",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.2",
    "@tanstack/react-table": "^8.20.5",
    "@types/node": "22.5.5",
    "@types/react": "18.3.8",
    "@types/react-dom": "18.3.0",
    "autoprefixer": "10.4.20",
    "axios": "^1.7.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "1.0.0",
    "concurrently": "^9.0.1",
    "eslint": "8.41.0",
    "eslint-config-next": "13.4.4",
    "fast-xml-parser": "^4.5.0",
    "html2canvas-pro": "^1.5.8",
    "lucide-react": "^0.445.0",
    "moment": "^2.30.1",
    "next": "^14.2.13",
    "next-themes": "^0.3.0",
    "postcss": "^8.4.47",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.53.0",
    "react-pdf": "^9.1.1",
    "sweetalert2": "^11.14.1",
    "sweetalert2-react-content": "^5.0.7",
    "tailwind-merge": "^2.5.2",
    "tailwindcss": "3.4.12",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "5.6.2",
    "zod": "^3.23.8",
    "zustand": "^5.0.0-rc.2"
  },
  "devDependencies": {
    "prettier": "^3.3.3"
  }
}
